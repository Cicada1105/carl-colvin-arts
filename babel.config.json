{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "edge": "17",
          "firefox": "60",
          "chrome": "67",
          "safari": "11.1",
        }
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver", {
        "alias": {
          "@global": "./lib/es5/global"
        }
      }
    ]
  ]
}
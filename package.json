{
  "name": "carl-colvin-arts",
  "version": "1.0.0",
  "description": "Portfolio for Carl Colvin Arts",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "less": "^3.10.3",
    "typescript": "^3.7.5"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run-script compile;npm run-script transpile; npm run-script build; npm run-script dev",
    "compile": "for fileName in ./src/style/pgs/*.less ./src/style/pgs/services/*.less; do baseName=`basename ${fileName}`; base=${baseName%'.less'}; ./node_modules/less/bin/lessc $fileName ./dist/styles/$base.css; done",
    "transpile": "./node_modules/typescript/bin/tsc",
    "build": "./node_modules/.bin/babel ./lib/es6 --ignore ./lib/declarations --out-dir ./lib/es5/",
    "dev":"browserify ./lib/es5/ --outfile ./dist/index.js --standalone begin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cicada1105/carl-colvin-arts.git"
  },
  "keywords": [
    "Oboe",
    "Music",
    "Reeds",
    "Reedmaking",
    "Poetry",
    "Poems",
    "Performance",
    "Editing",
    "Writing"
  ],
  "author": "Josh Colvin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Cicada1105/carl-colvin-arts/issues"
  },
  "homepage": "https://github.com/Cicada1105/carl-colvin-arts#readme"
}
